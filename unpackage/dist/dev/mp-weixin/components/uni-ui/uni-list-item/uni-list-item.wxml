<view class="{{['uni-list-item',disabled?'uni-list-item--disabled':'']}}" hover-start-time="{{0}}" hover-stay-time="{{0}}" hover-class="{{disabled||showSwitch?'':'uni-list-item--hover'}}" data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" catchtap="__e"><view class="uni-list-item__container"><block wx:if="{{thumb}}"><view class="uni-list-item__icon"><image class="uni-list-item__icon-img" src="{{thumb}}"></image></view></block><block wx:else><block wx:if="{{showExtraIcon}}"><view class="uni-list-item__icon"><uni-icon class="uni-icon-wrapper" vue-id="0931e88e-1" color="{{extraIcon.color}}" size="{{extraIcon.size}}" type="{{extraIcon.type}}" bind:__l="__l"></uni-icon></view></block></block><view class="uni-list-item__content"><block wx:if="{{leftIcon}}"><view class="{{['iconfont',leftIcon]}}" style="{{'margin-right:20rpx;'+('color:'+(iconColor)+';')+('font-size:'+(leftIconSize)+';')}}"></view></block><view class="{{['uni-list-item__content-title',titleColor]}}">{{title}}</view><block wx:if="{{note}}"><view class="uni-list-item__content-note">{{note}}</view></block><slot></slot></view><block wx:if="{{showBadge||showArrow||showSwitch}}"><view class="uni-list-item__extra" style="{{('width:'+extraWidth+';')}}"><block wx:if="{{showBadge}}"><uni-badge vue-id="0931e88e-2" type="{{badgeType}}" text="{{badgeText}}" bind:__l="__l"></uni-badge></block><block wx:if="{{showSwitch}}"><switch disabled="{{disabled}}" checked="{{switchChecked}}" data-event-opts="{{[['change',[['onSwitchChange',['$event']]]]]}}" bindchange="__e"></switch></block><block wx:if="{{rightIcon}}"><view class="{{['iconfont',rightIcon]}}" style="{{'color:'+(rightIconColor)+';'}}"></view></block><block wx:if="{{rightText}}"><view class="{{[rightTextColor]}}">{{rightText}}</view></block><block wx:if="{{showArrow}}"><slot name="icon"></slot></block></view></block></view></view>